<UIX xmlns="http://schemas.microsoft.com/2007/uix"
     xmlns:iris="assembly://UIX/Microsoft.Iris">
    <UI Name="Default">
        <Locals>

            <iris:ArrayListDataSet Name="ArraySet">
                <Source>
                    <Int32 Int32="10"/>
                    <Int32 Int32="20"/>
                    <Int32 Int32="30"/>
                </Source>
            </iris:ArrayListDataSet>
        </Locals>

        <Content>
            <Text Color="Red">
                <Content>Howdy from Microsoft.Iris!</Content>
            </Text>
        </Content>

        <Scripts>
            <Script><![CDATA[
                int sum = 0;
				
				foreach (Int32 num in ArraySet)
                {
				    int y = num;
					
					if (y % 2 == 0)
					{
					    y = y / 2;
					}
					else
					{
                        y = y * 3;
					}
					
                    sum = sum + y;
                }
				
				int x = sum * 5;
            ]]></Script>
        </Scripts>
    </UI>
</UIX>